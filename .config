# Mysql executable path
mysql=/Applications/MAMP/Library/bin/mysql
# Mysql username
dbuser=root
# Mysql Password
dbpass=root
# PHP executable path
php=/Applications/MAMP/bin/php/php7.4.16/bin/php

# Apache restart & vhost paths
vhost_config=/Applications/MAMP/conf/apache/extra/httpd-vhosts.conf
stopApache=/Applications/MAMP/bin/stopApache.sh
startApache=/Applications/MAMP/bin/startApache.sh

# Set the color variable
warn='\033[0;33m'
green='\033[0;32m'
red='\033[0;31m'
clear='\033[0m'
